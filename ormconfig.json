{
  "type": "postgres",
  "name": "default",
  "host": "localhost",
  "port": 5432,
  "synchronize": false,
  "logging": false,
  "entities": ["src/pages/api/_server/entities/**/*{.ts,.js}"],
  "migrations": ["src/pages/api/_server/migrations/**/*{.ts,.js}"],
  "seeds": ["src/pages/api/_server/db/seeding/seeds/**/*{.ts,.js}"],
  "cli": {
    "entitiesDir": "src/pages/api/_server/entities",
    "migrationsDir": "src/pages/api/_server/migrations"
  },
  "formatOptions": {
    "enableUuidHack": true
  }
}
