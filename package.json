{
  "name": "y_sure-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "prepare": "husky install",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "typeorm:cli": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn typeorm:cli migration:generate -n",
    "typeorm:migration:revert": "yarn typeorm:cli migration:revert",
    "typeorm:migration:run": "yarn typeorm:cli migration:run"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "jest --findRelatedTests --forceExit"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "antd": "^5.11.1",
    "classnames": "^2.3.2",
    "next": "14.0.2",
    "pg": "^8.11.3",
    "react": "^18",
    "react-dom": "^18",
    "swr": "^2.2.4",
    "tailwind-scrollbar": "^3.0.5",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "autoprefixer": "^10.0.1",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^14.0.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "postcss": "^8",
    "prettier": "^3.1.0",
    "reflect-metadata": "^0.1.13",
    "tailwindcss": "^3.3.5",
    "typescript": "^5"
  },
  "engines": {
    "node": "18.18.2"
  }
}
