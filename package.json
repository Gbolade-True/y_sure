{
  "name": "y_sure",
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "postinstall": "tsc",
    "build": "tsc",
    "serve": "ts-node ./dist/src/index.ts",
    "start": "yarn serve",
    "watch": "nodemon index.ts",
    "watch-ts": "tsc -w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "typeorm:cli": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn typeorm:cli migration:generate -n",
    "typeorm:migration:revert": "yarn typeorm:cli migration:revert",
    "typeorm:migration:run": "yarn typeorm:cli migration:run"
  },
  "license": "MIT",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/node": "^20.3.3",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "reflect-metadata": "^0.1.13"
  },
  "engines": {
    "node": "16.20.1"
  }
}
